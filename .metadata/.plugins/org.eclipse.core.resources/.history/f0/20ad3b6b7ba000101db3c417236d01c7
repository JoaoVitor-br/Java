package PrimeiroProjeto;

public class FuncaoSimples {

	public static void main(String[] args) {
		mensagem();
		comtagem();

	}
	
	public static void mensagem() {
		System.out.println("Primeira função");
		
	}
	
	public static void comtagem() {
		for(int e =1;1<10;e++) {
			System.out.println(e);
		}
	}

}

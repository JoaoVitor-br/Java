package PrimeiroProjeto;

public class Lista20 {

	public static void main(String[] args) {
		
		for(int e =0;e <5;e++) {
			Bia();
		}for(int e = 0; e < 6; e++){
			retangolo();
		}


	}
	public static void Bia() {
		System.out.println("Bom dia!");
	}
	public static void retangolo() {
		System.out.print("************\n");
	}


}
